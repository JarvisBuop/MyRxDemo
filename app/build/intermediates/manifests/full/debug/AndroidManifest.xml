<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjy.myrxdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.zjy.myrxdemo.MyRxApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:vmSafeMode="true" >
        <activity android:name="com.zjy.myrxdemo.MainActivity" >
        </activity>
        <activity
            android:name="com.zjy.myrxdemo.business.login.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zjy.myrxdemo.business.register.RegisterActivity"
            android:theme="@style/Translucent" >
        </activity>
        <activity
            android:name="com.antfortune.freeline.MiddlewareActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.antfortune.freeline.FreelineService"
            android:exported="true"
            android:process=":freeline" />
        <service
            android:name="com.antfortune.freeline.FreelineService$InnerService"
            android:process=":freeline" />

        <receiver android:name="com.antfortune.freeline.FreelineReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.FreelineReceiver" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>
    </application>

</manifest>